{
  "extends": "airbnb",
  "emitWarning": true,
  "quiet": true,
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": 0
 //   "semi": [2, "never"],
/*
    "eol-last": 0,
    "no-new": 0,
    "semi": 0,
    "no-tabs": 0,
    "no-trailing-spaces": 0,
    "space-before-blocks": 0,
    "indent": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 0,

    "spaced-comment": 0,
    "comma-dangle": 0,
    "no-mixed-spaces-and-tabs": 0,

    "react/prefer-stateless-function": 0,
    "react/jsx-indent": 0,
    "react/jsx-tag-spacing": 0,
    "react/jsx-space-before-closing": 0,
    "react/self-closing-comp": 0,

    "object-curly-spacing": 0,
    "comma-spacing": 0,
    "key-spacing": 0,
    "prefer-template": 0,
    "no-console": 0,
    "quotes": 0,

    "space-infix-ops": 0,
    "padded-blocks": 0,
    "no-multiple-empty-lines": 0,
    "prefer-const": 0,
    "no-extra-semi": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0,

    "no-undef": 0,
    "quote-props": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "space-in-parens": 0,
    "no-underscore-dangle": 0,
    "camelcase": 0,

    "object-shorthand": 0,
    "new-cap": 0,
    "react/sort-comp": 0,
    "prefer-arrow-callback": 0,
    "react/jsx-wrap-multilines": 0,
    "arrow-parens": 0,
    "arrow-spacing": 0,
    "no-return-assign": 0,
    "no-constant-condition": 0,
    "jsx-a11y/href-no-hash": 0,
    "keyword-spacing": 0,

    "no-mixed-operators": 0,
    "import/no-extraneous-dependencies": 0,
    "import/first": 0,
    "no-multi-spaces": 0,
    //"import/no-duplicates": 0,
    //"no-redeclare": 0,
    //"react/jsx-no-undef": 0,
    /*
    "": 0,
    "": 0,

    "EOL": 0  //not much usefull

*/
  }
}